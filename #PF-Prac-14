def rotate_list(input_list,n):
    #start writing your code here
    length = len(input_list)
    output_list=[0]*length
    j=-1 
    while j != -length-1:
        output_list[j+n]=input_list[j]
        j-=1 

    return output_list

input_list= [1,2,3,4,5,6]
output_list=rotate_list(input_list,4)
print(output_list)
